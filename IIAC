local rep = game:GetService("ReplicatedStorage"):GetDescendants()

for i,v in ipairs(rep) do

    if v:IsA("BoolValue") then
        v.Value = true
        end

        if v:IsA("StringValue") then
            v.Value = 1000000000000000000
            end

            if v:IsA("IntValue") then
                v.Value = 1000000000000000000
                end
wait(0.1)
end
wait(0.5)
game:GetService("Players").LocalPlayer:Kick("Your Account is Succesfully Modded!, Rejoin")
